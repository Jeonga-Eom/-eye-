<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".KimActivity_plus">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Left \n eye"
        android:textSize="40dp"
        android:layout_x="80dp"
        android:layout_y="130dp"/>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Right \n eye"
        android:textSize="40dp"
        android:layout_x="237dp"
        android:layout_y="130dp" />

    <EditText
        android:id="@+id/leftlens"
        android:background="@drawable/rect_text_edit"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:hint="왼쪽시력"
        android:gravity="center"
        android:layout_x="80dp"
        android:layout_y="250dp"/>
    <EditText
        android:id="@+id/rightlens"
        android:background="@drawable/rect_text_edit"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:hint="오른쪽시력"
        android:gravity="center"
        android:layout_x="237dp"
        android:layout_y="250dp"/>\

    <Button
        android:id="@+id/before1"
        android:layout_width="107dp"
        android:layout_height="96dp"
        android:layout_x="80dp"
        android:layout_y="394dp"
        android:text="취소" />

    <Button
        android:id="@+id/next1"
        android:layout_width="107dp"
        android:layout_height="96dp"
        android:layout_x="237dp"
        android:layout_y="394dp"
        android:text="Next" />
</AbsoluteLayout>